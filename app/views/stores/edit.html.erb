<h1>Store#edit</h1>
<p>Find me in app/views/store/edit.html.erb</p>

<%= form_tag({controller: "stores", action: "save", id: @store.id}, {method: "put"}) do %>
  <label>Name</label>
	<%= text_field_tag("name", @store.name ) %>
  <%= text_field_tag("town", @store.town ) %>
  <p>
  <%= submit_tag("Save!") %>
<% end %>

